{
	"apiVersion": "v1",
	"kind": "Service",
	"metadata": {
		"name": "zookeeper",
		"labels": {
			"app": "zookeeper"
		}
	},
	"spec": {
		"ports": [
			{
				"port": 2888,
				"name": "server"
			},
			{
				"port": 3888,
				"name": "leader-election"
			},
			{
				"port": 2181,
				"name": "client"
			}
		],
		"clusterIP": "None",
		"selector": {
			"app": "zookeeper"
		}
	}
}